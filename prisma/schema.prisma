
generator client {
  provider = "prisma-client-js"
}

model Item {
  id    String @id @default(uuid()) // @id:dynamic
  name        String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid()) // @id:dynamic
  email String @unique
  name  String?
}
